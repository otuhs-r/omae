<h1>登録情報ンゴ</h1>
<table>
  <tr>
    <th>名前</th>
    <td><%= @user_name %></td>
  </tr>
  <tr>
    <th>メールアドレス</th>
    <td><%= @email %></td>
  </tr>
  <tr>
    <th>始業時刻</th>
    <td><%= @work_start_time.to_s(:time) %></td>
  </tr>
  <tr>
    <th>終業時刻</th>
    <td><%= @work_end_time.to_s(:time) %></td>
  </tr>
  <tr>
    <th>休憩開始時刻</th>
    <td><%= @rest_start_time.to_s(:time) %></td>
  </tr>
  <tr>
    <th>休憩終了時刻</th>
    <td><%= @rest_end_time.to_s(:time) %></td>
  </tr>
</table>
